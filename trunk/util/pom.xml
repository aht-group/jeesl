	        	<artifactId>exlp-util</artifactId>
	        	<version>${exlp.version}</version>
	        	<type>jar</type>
	        	<scope>compile</scope>
	        	<exclusions>
	        		<exclusion>
	        			<artifactId>commons-collections</artifactId>
	        			<groupId>commons-collections</groupId>
	        		</exclusion>
	        	</exclusions>
	        </dependency>
	        <dependency>
	        	<groupId>net.sf.ahtutils</groupId>
