        </component>
    </tag>
    <tag>
        <tag-name>fullHdGrid</tag-name>
        <component>
            <component-type>net.sf.ahtutils.prototype.web.component.FullHdGrid</component-type>
        </component>
    </tag>
    <tag>
        <tag-name>hdGrid</tag-name>
        <component>
            <component-type>net.sf.ahtutils.prototype.web.component.HdGrid</component-type>
        </component>
    </tag>

</facelet-taglib> 