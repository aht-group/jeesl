<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.jeesl.org/inventory"
	xmlns:i="http://www.jeesl.org/inventory"
    xmlns:pc="http://www.jeesl.org/inventory/pc"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://www.jeesl.org/inventory/pc" schemaLocation="pc.xsd"/>
    <xsd:element name="inventory">
	    <xsd:complexType>
            <xsd:sequence>
	            <xsd:element ref="pc:computer" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>    
    </xsd:element>
</xsd:schema>