<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://ahtutils.aht-group.com/access"
    xmlns:a="http://ahtutils.aht-group.com/access"
    xmlns:s="http://ahtutils.aht-group.com/status"
    xmlns:sec="http://ahtutils.aht-group.com/security"
    xmlns:n="http://ahtutils.aht-group.com/navigation"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://ahtutils.aht-group.com/status" schemaLocation="../jeesl/status.xsd"/>
    <xsd:import namespace="http://ahtutils.aht-group.com/security" schemaLocation="security.xsd"/>
    <xsd:import namespace="http://ahtutils.aht-group.com/navigation" schemaLocation="navigation.xsd"/>
   
    <xsd:element name="access">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="a:category" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
   
    <xsd:element name="category">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="s:langs" maxOccurs="1"/>
                <xsd:element ref="s:descriptions" maxOccurs="1"/>
                <xsd:element ref="sec:roles" maxOccurs="1"/>
                <xsd:element ref="a:views" maxOccurs="1"/>
            </xsd:sequence>
            <xsd:attribute name="code" type="xsd:string"/>
            <xsd:attribute name="label" type="xsd:string"/>
            <xsd:attribute name="position" type="xsd:int"/>
            <xsd:attribute name="visible" type="xsd:boolean"/>
            <xsd:attribute name="documentation" type="xsd:boolean"/>
        </xsd:complexType>
    </xsd:element>
   
   
    <xsd:element name="views">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="a:view" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="view">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="s:descriptions" maxOccurs="1"/>
                <xsd:element ref="s:langs" maxOccurs="1"/>
                <xsd:element ref="sec:actions" maxOccurs="1"/>
                <xsd:element ref="sec:roles" maxOccurs="1"/>
                <xsd:element ref="n:navigation" maxOccurs="1"/>
            </xsd:sequence>
            <xsd:attribute name="code" type="xsd:string"/>
            <xsd:attribute name="public" type="xsd:boolean"/>
            <xsd:attribute name="onlyLoginRequired" type="xsd:boolean"/>
            <xsd:attribute name="urlParameter" type="xsd:string"/>
            <xsd:attribute name="label" type="xsd:string"/>
            <xsd:attribute name="position" type="xsd:int"/>
            <xsd:attribute name="visible" type="xsd:boolean"/>
            <xsd:attribute name="documentation" type="xsd:boolean"/>
        </xsd:complexType>
    </xsd:element>
 
</xsd:schema>